<% if(success && success.length) {%> 
    <div class="alert alert-success alert-dismissible fade show d-flex flex-row justify-content-between" role="alert">
      <%= success %>
      <span type="button" class="close" data-bs-dismiss="alert" aria-label="Close">&times;</span>
    </div>
    <% } %>

    <% if(error && error.length) {%>
      <div class="alert alert-danger alert-dismissible fade show d-flex flex-row justify-content-between" role="alert">
          <%= error %>
            <span type="button" class="close" data-bs-dismiss="alert" aria-label="Close">
              <span aria-hidden="true">&times;</span>
          </span>
      </div>
      <% } %>


